cus.id NIT, cus.customerName, sst.saleStatusName Estado,
sum(sde.productSaleQuantity * sde.productSalePrice) - sum(1 * sde.productSaleDiscount) Monto
from sale_details as sde
inner join sales as sal
on sde.sale_id = sal.id
inner join customers as cus
on sal.customer_id = cus.id
inner join sale_statuses as sst
on sal.saleStatus_id = sst.id
where sst.saleStatusName = 'FINALIZADA'
